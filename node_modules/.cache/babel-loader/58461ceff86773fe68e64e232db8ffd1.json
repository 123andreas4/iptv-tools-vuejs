AAA,IAAA;AACA;AAdA,GATA;AAyBA,EAAA,OAAA,kCACA,UAAA,CAAA,CAAA,oBAAA,EAAA,yBAAA,CAAA,CADA;AAEA,IAAA,MAFA,oBAEA;AACA,MAAA,QAAA,CAAA,KAAA,CAAA,cAAA;AACA,KAJA;AAKA,IAAA,UALA,sBAKA,IALA,EAKA;AACA,WAAA,IAAA,GAAA,IAAA;AACA,KAPA;AAQA,IAAA,QARA,oBAQA,EARA,EAQA;AACA,WAAA,EAAA,GAAA,EAAA;AACA,KAVA;AAWA,IAAA,WAXA,uBAWA,KAXA,EAWA;AACA,WAAA,KAAA,GAAA,KAAA;AACA,KAbA;AAcA,IAAA,YAdA,wBAcA,MAdA,EAcA;AACA,WAAA,MAAA,GAAA,MAAA;AACA;AAhBA,IAzBA;AA2CA,EAAA,KAAA,EAAA;AACA,IAAA,MAAA,EAAA,kBAAA;AACA,WAAA,kBAAA;AACA,WAAA,EAAA,GAAA,CAAA;AACA,WAAA,IAAA,GAAA,CAAA;AACA,WAAA,KAAA,GAAA,CAAA;AACA,WAAA,SAAA,GAAA,CAAA;AACA,WAAA,cAAA,GAAA,SAAA;AACA,WAAA,MAAA,GAAA,KAAA;;AACA,UAAA,KAAA,MAAA,CAAA,MAAA,CAAA,MAAA,KAAA,IAAA,EAAA;AACA,aAAA,MAAA,GAAA,KAAA;AACA;AACA,KAZA;AAaA,IAAA,SAAA,EAAA,mBAAA,GAAA,EAAA;AACA,MAAA,QAAA,CAAA,KAAA,CAAA,YAAA,EAAA,GAAA;AACA;AAfA,GA3CA;AA4DA,EAAA,WA5DA,yBA4DA;AACA,SAAA,kBAAA;AACA,SAAA,uBAAA;AACA,IAAA,QAAA,CAAA,GAAA,CAAA,aAAA,EAAA,KAAA,UAAA;AACA,IAAA,QAAA,CAAA,GAAA,CAAA,WAAA,EAAA,KAAA,QAAA;AACA,IAAA,QAAA,CAAA,GAAA,CAAA,cAAA,EAAA,KAAA,WAAA;AACA,IAAA,QAAA,CAAA,GAAA,CAAA,eAAA,EAAA,KAAA,YAAA;AACA,GAnEA;AAoEA,EAAA,aApEA,2BAoEA;AACA,IAAA,QAAA,CAAA,IAAA,CAAA,aAAA,EAAA,KAAA,UAAA;AACA,IAAA,QAAA,CAAA,IAAA,CAAA,WAAA,EAAA,KAAA,QAAA;AACA,IAAA,QAAA,CAAA,IAAA,CAAA,cAAA,EAAA,KAAA,WAAA;AACA,IAAA,QAAA,CAAA,IAAA,CAAA,eAAA,EAAA,KAAA,YAAA;AACA;AAzEA,CAAA","sourcesContent":["<template>\n  <erd-row>\n    <erd-breadcrumb :items=\"breadcrumb\" :heading=\"heading\">\n      <erd-button class=\"float-right\" icon=\"la-angle-left\" v-if=\"viewer\" @click=\"doBack\" >{{ $t(\"general.back\") }}</erd-button>\n      <span class=\"text-muted\" v-if=\"!viewer\">{{\n        $t(\"general.from-to-total\").format(\n          this.total > 0 ? this.from + 1 : this.from,\n          this.to,\n          this.total\n        )\n      }}</span>\n    </erd-breadcrumb>\n    <div class=\"seperator page\"></div>\n    <router-view class=\"px-0 mt-0\"></router-view>\n  </erd-row>\n</template>\n\n<script>\n/* eslint-disable no-unused-vars */\nimport { EventBus } from \"../../../services/ebus\";\nimport { mapActions } from \"vuex\";\nexport default {\n  data() {\n    return {\n      viewer: false,\n      from: 0,\n      to: 0,\n      total: 0,   \n    };\n  },\n  computed: {\n    breadcrumb() {\n      let res = [];\n      this.$route.matched.map((item) => {\n        res.push({\n          text: this.$t(`menu.${item.name.toLowerCase()}`),\n          to: item.path !== \"\" ? item.path : \"/\",\n        });\n      });\n      return res;\n    },\n    heading() {\n      let br = this.breadcrumb;\n      return br[br.length - 1].text;\n    },\n  },\n  methods: {\n    ...mapActions([\"loadxdProInstances\", \"loadMovieSeriePlaylists\"]),\n    doBack() {\n      EventBus.$emit(\"close-viewer\");\n    },\n    updateFrom(from) {\n      this.from = from;\n    },\n    updateTo(to) {\n      this.to = to;\n    },\n    updateTotal(total) {\n      this.total = total;\n    },\n    updateViewer(viewer) {\n      this.viewer = viewer;\n    }\n  },\n  watch: {\n    $route: function () {\n      this.loadxdProInstances();\n      this.to = 0;\n      this.from = 0;\n      this.total = 0;\n      this.activeTab = 0;\n      this.editorPlaylist = undefined;\n      this.viewer = false;\n      if (this.$route.params.editor !== true) {\n        this.editor = false;\n      }\n    },\n    activeTab: function (tab) {\n      EventBus.$emit(\"tab-change\", tab);\n    },\n  },\n  beforeMount() {\n    this.loadxdProInstances();\n    this.loadMovieSeriePlaylists();\n    EventBus.$on(\"update-from\", this.updateFrom);\n    EventBus.$on(\"update-to\", this.updateTo);\n    EventBus.$on(\"update-total\", this.updateTotal);\n    EventBus.$on(\"update-viewer\", this.updateViewer);\n  },\n  beforeDestroy() {\n    EventBus.$off(\"update-from\", this.updateFrom);\n    EventBus.$off(\"update-to\", this.updateTo);\n    EventBus.$off(\"update-total\", this.updateTotal);\n    EventBus.$off(\"update-viewer\", this.updateViewer);\n  },\n};\n</script>\n\n<style scoped>\n.seperator.page {\n  margin-left: 15px;\n  margin-right: 15px;\n  width: calc(100% - 30px);\n}\n.playlist-select {\n  min-width: 200px;\n}\n</style>\n"],"sourceRoot":"src/views/app/movies"}]}