A,aAAA,YAAA;AACA;AACA;AAXA,GA5BA;AAyCA,EAAA,aAzCA,2BAyCA;AACA,SAAA,YAAA;AACA;AA3CA,CAAA","sourcesContent":["<template>\n  <div\n    :class=\"{ 'app-menu': true, shown: isOpen }\"\n    @mouseenter=\"isMenuOver = true\"\n    @mouseleave=\"isMenuOver = false\"\n  >\n    <slot></slot>\n    <a class=\"app-menu-button d-inline-block d-xl-none\" @click.prevent=\"toggle\"\n      ><i class=\"las\" :class=\"icon\"\n    /></a>\n  </div>\n</template>\n\n<script>\nimport { setTimeout } from \"timers\";\nexport default {\n  props: {\n    icon: String,\n  },\n  data() {\n    return {\n      isOpen: false,\n      isMenuOver: false,\n    };\n  },\n  methods: {\n    addEvents() {\n      document.addEventListener(\"click\", this.handleDocumentClick);\n      document.addEventListener(\"touchstart\", this.handleDocumentClick);\n    },\n    removeEvents() {\n      document.removeEventListener(\"click\", this.handleDocumentClick);\n      document.removeEventListener(\"touchstart\", this.handleDocumentClick);\n    },\n    handleDocumentClick() {\n      if (!this.isMenuOver) {\n        this.toggle();\n      }\n    },\n    toggle() {\n      this.isOpen = !this.isOpen;\n    },\n  },\n  watch: {\n    isOpen(val) {\n      if (val) {\n        setTimeout(() => {\n          this.$emit(\"show\");\n        }, 300);\n\n        this.addEvents();\n      } else {\n        this.removeEvents();\n      }\n    },\n  },\n  beforeDestroy() {\n    this.removeEvents();\n  },\n};\n</script>\n"],"sourceRoot":"src/components/App"}]}