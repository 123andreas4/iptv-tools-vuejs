dA,qBAcA;AACA,UAAA,EAAA,GAAA,KAAA,UAAA;AACA,aAAA,EAAA,CAAA,EAAA,CAAA,MAAA,GAAA,CAAA,CAAA,CAAA,IAAA;AACA,KAjBA;AAkBA,IAAA,aAlBA,2BAkBA;AACA;AACA,UAAA,iBAAA,IAAA,CAAA,KAAA,MAAA,CAAA,IAAA,CAAA,EAAA;AACA,eAAA,KAAA,EAAA,CAAA,iBAAA,CAAA;AACA,OAJA,CAKA;;;AACA,UAAA,qBAAA,IAAA,CAAA,KAAA,MAAA,CAAA,IAAA,CAAA,EAAA;AACA,eAAA,KAAA,EAAA,CAAA,wBAAA,CAAA;AACA;;AAEA,aAAA,EAAA;AACA;AA7BA,GATA;AAwCA,EAAA,OAAA,EAAA;AACA,IAAA,UADA,sBACA,IADA,EACA;AACA,WAAA,IAAA,GAAA,IAAA;AACA,KAHA;AAIA,IAAA,QAJA,oBAIA,EAJA,EAIA;AACA,WAAA,EAAA,GAAA,EAAA;AACA,KANA;AAOA,IAAA,WAPA,uBAOA,KAPA,EAOA;AACA,WAAA,KAAA,GAAA,KAAA;AACA,KATA;AAUA,IAAA,YAVA,wBAUA,MAVA,EAUA;AACA,WAAA,MAAA,GAAA,MAAA;AACA,KAZA;AAaA,IAAA,SAbA,uBAaA;AACA,MAAA,QAAA,CAAA,KAAA,CAAA,SAAA;AACA,KAfA;AAgBA,IAAA,QAhBA,sBAgBA;AACA,MAAA,QAAA,CAAA,KAAA,CAAA,QAAA;AACA,KAlBA;AAmBA,IAAA,MAnBA,oBAmBA;AACA,MAAA,QAAA,CAAA,KAAA,CAAA,MAAA;AACA,KArBA;AAsBA,IAAA,KAtBA,mBAsBA;AACA,MAAA,QAAA,CAAA,KAAA,CAAA,KAAA;AACA;AAxBA,GAxCA;AAkEA,EAAA,KAAA,EAAA;AACA,IAAA,MAAA,EAAA,kBAAA;AACA,WAAA,EAAA,GAAA,CAAA;AACA,WAAA,IAAA,GAAA,CAAA;AACA,WAAA,KAAA,GAAA,CAAA;;AACA,UAAA,KAAA,MAAA,CAAA,MAAA,CAAA,MAAA,KAAA,IAAA,EAAA;AACA,aAAA,MAAA,GAAA,KAAA;AACA;AACA;AARA,GAlEA;AA4EA,EAAA,WA5EA,yBA4EA;AACA,IAAA,QAAA,CAAA,GAAA,CAAA,aAAA,EAAA,KAAA,UAAA;AACA,IAAA,QAAA,CAAA,GAAA,CAAA,WAAA,EAAA,KAAA,QAAA;AACA,IAAA,QAAA,CAAA,GAAA,CAAA,cAAA,EAAA,KAAA,WAAA;AACA,IAAA,QAAA,CAAA,GAAA,CAAA,eAAA,EAAA,KAAA,YAAA;AACA,GAjFA;AAkFA,EAAA,aAlFA,2BAkFA;AACA,IAAA,QAAA,CAAA,IAAA,CAAA,aAAA,EAAA,KAAA,UAAA;AACA,IAAA,QAAA,CAAA,IAAA,CAAA,WAAA,EAAA,KAAA,QAAA;AACA,IAAA,QAAA,CAAA,IAAA,CAAA,cAAA,EAAA,KAAA,WAAA;AACA,IAAA,QAAA,CAAA,IAAA,CAAA,eAAA,EAAA,KAAA,YAAA;AACA;AAvFA,CAAA","sourcesContent":["<template>\n  <erd-row>\n    <erd-breadcrumb :items=\"breadcrumb\" :heading=\"heading\">\n      <erd-button\n        class=\"mr-1 py-1 px-2\"\n        icon=\"las la-save\"\n        @click=\"doSave\"\n        v-if=\"editor\"\n        variant=\"success\"\n        >{{ $t(\"general.save\") }}</erd-button\n      >\n      <erd-button\n        class=\"py-1 px-2\"\n        icon=\"las la-ban\"\n        @click=\"doCancel\"\n        v-if=\"editor\"\n        variant=\"danger\"\n        >{{ $t(\"general.cancel\") }}</erd-button\n      >\n      <erd-button\n        class=\"mr-1 py-1 px-2\"\n        icon=\"las la-plus\"\n        @click=\"doAdd\"\n        v-if=\"!editor && !hideButtons\"\n        variant=\"success\"\n        >{{ addButtonText }}</erd-button\n      >\n      <erd-button\n        class=\"mr-2 py-1 px-2\"\n        icon=\"las la-sync\"\n        @click=\"doRefresh\"\n        v-if=\"!editor && !hideButtons\"\n        >{{ $t(\"general.refresh\") }}</erd-button\n      >\n      <span class=\"text-muted\" v-if=\"!editor && !hideButtons\">{{\n        $t(\"general.from-to-total\").format(\n          this.total > 0 ? this.from + 1 : this.from,\n          this.to,\n          this.total\n        )\n      }}</span>\n    </erd-breadcrumb>\n    <div class=\"seperator page\"></div>\n    <router-view class=\"px-0 mt-0\"></router-view>\n  </erd-row>\n</template>\n\n<script>\nimport { EventBus } from \"../../../services/ebus\";\nexport default {\n  data() {\n    return {\n      from: 0,\n      to: 0,\n      total: 0,\n      editor: false,\n    };\n  },\n  computed: {\n    hideButtons() {\n      return /\\/tools\\/terminal/.test(this.$route.path);\n    },\n    breadcrumb() {\n      let res = [];\n      this.$route.matched.map((item) => {\n        res.push({\n          text: this.$t(`menu.${item.name.toLowerCase()}`),\n          to: item.path !== \"\" ? item.path : \"/\",\n        });\n      });\n      return res;\n    },\n    heading() {\n      let br = this.breadcrumb;\n      return br[br.length - 1].text;\n    },\n    addButtonText() {\n      // XMLTV\n      if (/\\/tools\\/xmltv/.test(this.$route.path)) {\n        return this.$t(\"xmltv.add-xmltv\");\n      }\n      // Scheduler\n      if (/\\/tools\\/scheduler/.test(this.$route.path)) {\n        return this.$t(\"scheduler.add-schedule\");\n      }\n\n      return \"\";\n    },\n  },\n  methods: {\n    updateFrom(from) {\n      this.from = from;\n    },\n    updateTo(to) {\n      this.to = to;\n    },\n    updateTotal(total) {\n      this.total = total;\n    },\n    updateEditor(editor) {\n      this.editor = editor;\n    },\n    doRefresh() {\n      EventBus.$emit(\"refresh\");\n    },\n    doCancel() {\n      EventBus.$emit(\"cancel\");\n    },\n    doSave() {\n      EventBus.$emit(\"save\");\n    },\n    doAdd() {\n      EventBus.$emit(\"add\");\n    },\n  },\n  watch: {\n    $route: function () {\n      this.to = 0;\n      this.from = 0;\n      this.total = 0;\n      if (this.$route.params.editor !== true) {\n        this.editor = false;\n      }\n    },\n  },\n  beforeMount() {\n    EventBus.$on(\"update-from\", this.updateFrom);\n    EventBus.$on(\"update-to\", this.updateTo);\n    EventBus.$on(\"update-total\", this.updateTotal);\n    EventBus.$on(\"update-editor\", this.updateEditor);\n  },\n  beforeDestroy() {\n    EventBus.$off(\"update-from\", this.updateFrom);\n    EventBus.$off(\"update-to\", this.updateTo);\n    EventBus.$off(\"update-total\", this.updateTotal);\n    EventBus.$off(\"update-editor\", this.updateEditor);\n  },\n};\n</script>\n\n<style scoped>\n.seperator.page {\n  margin-left: 15px;\n  margin-right: 15px;\n  width: calc(100% - 30px);\n}\n</style>\n"],"sourceRoot":"src/views/app/app-sources"}]}