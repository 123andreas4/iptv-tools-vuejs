ACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"dashboard.vue","sourceRoot":"src/views/app/start","sourcesContent":["<template>\n  <erd-row class=\"m-0 p-0\" id=\"dashboard\">\n    <erd-spinner v-if=\"isLoading\" size=\"lg\" overlay absolute></erd-spinner>\n    <erd-col xl=\"12\" xs=\"12\" class=\"m-0 p-0\">\n      <erd-row class=\"m-0 p-0\">\n        <erd-col xl=\"3\" md=\"6\">\n          <erd-card>\n            <erd-card-body class=\"dashboard-card\">\n              <i class=\"las la-calendar-day la-5x color-theme-1\"></i>\n              <div class=\"dashboard-card-title\" style=\"\">\n                <h5 class=\"m-0\">{{ $t(\"profile.member-since\") }}</h5>\n                <p class=\"m-0 color-theme-1\">{{ memberSince }}</p>\n              </div>\n            </erd-card-body>\n          </erd-card>\n        </erd-col>\n        <erd-col xl=\"3\" md=\"6\">\n          <erd-card>\n            <erd-card-body class=\"dashboard-card\">\n              <i class=\"las la-user-cog la-5x color-theme-1\"></i>\n              <div class=\"dashboard-card-title\" style=\"\">\n                <h5 class=\"m-0\">{{ $t(\"profile.member-status\") }}</h5>\n                <p class=\"m-0 color-theme-1\">{{ userRole }}</p>\n              </div>\n            </erd-card-body>\n          </erd-card>\n        </erd-col>\n        <erd-col xl=\"3\" md=\"6\">\n          <erd-card>\n            <erd-card-body class=\"dashboard-card\">\n              <i class=\"las la-user-tag la-5x color-theme-1\"></i>\n              <div class=\"dashboard-card-title\" style=\"\">\n                <h5 class=\"m-0\">{{ $t(\"profile.subscription\") }}</h5>\n                <p class=\"m-0 color-theme-1\">{{ memberStatus }}</p>\n              </div>\n            </erd-card-body>\n          </erd-card>\n        </erd-col>\n        <erd-col xl=\"3\" md=\"6\">\n          <erd-card>\n            <erd-card-body class=\"dashboard-card\">\n              <i class=\"las la-file-invoice-dollar la-5x color-theme-1\"></i>\n              <div class=\"dashboard-card-title\" style=\"\">\n                <h5 class=\"m-0\">{{ $t(\"profile.next-invoice\") }}</h5>\n                <p class=\"m-0 text-danger\" v-if=\"nextInvoiceExpired\">\n                  {{ nextInvoiceDate }}\n                </p>\n                <p class=\"m-0 color-theme-1\" v-else>{{ nextInvoiceDate }}</p>\n              </div>\n            </erd-card-body>\n          </erd-card>\n        </erd-col>\n      </erd-row>\n    </erd-col>\n    <erd-col xl=\"12\" xs=\"12\" class=\"m-0 p-0\">\n      <erd-row class=\"m-0 p-0\">\n        <erd-col xl=\"6\" lg=\"12\" xs=\"12\">\n          <erd-card class=\"list-card\">\n            <erd-button\n              class=\"refresh-button\"\n              outline\n              @click=\"loadDashboard\"\n            >\n              <erd-tooltip :tooltip=\"$t('dashboard.tooltip-refresh')\" :enabled=\"showTooltips\">\n                <i class=\"las la-sync\"></i>\n              </erd-tooltip>\n            </erd-button>\n            <erd-card-body>\n              <div class=\"float-left float-none-xs\">\n                <div class=\"d-inline-block\">\n                  <h5 class=\"d-inline\">\n                    {{ $t(\"dashboard.xtream\") }}\n                    <span class=\"text-muted text-small text-capitalize\"></span>\n                  </h5>\n                  <span class=\"text-muted text-small d-block\">{{\n                    $t(\"dashboard.xtream-account-info\")\n                  }}</span>\n                </div>\n              </div>\n            </erd-card-body>\n            <erd-card-body class=\"list mt-1\">\n              <p\n                class=\"text-muted text-center mt-3\"\n                v-if=\"xtreamAccounts.length === 0\"\n              >\n                <i class=\"las la-globe d-block la-3x\"></i>\n                {{ $t(\"dashboard.no-xtream-accounts\") }}\n              </p>\n              <perfect-scrollbar>\n                <a\n                  class=\"dashboard-list-item\"\n                  v-for=\"(xtream, index) in xtreamAccounts\"\n                  :key=\"`xtream-account-${index}`\"\n                  href=\"#\"\n                  @click.prevent=\"xtreamAccountModal(true, xtream)\"\n                >\n                  <i class=\"las la-globe la-3x\"></i>\n                  <div class=\"details\">\n                    <p class=\"mt-1 float-right text-small\">\n                      {{ xtream.user_info.active_cons }}/{{\n                        xtream.user_info.max_connections\n                      }}\n                      <i class=\"las la-plug\"></i>\n                    </p>\n                    <p class=\"m-0 p-0\">\n                      <span>{{ xtream.server_info.url }}</span>\n                      <span class=\"text-muted text-small ml-2\">{{\n                        xtream.user_info.message\n                      }}</span>\n                    </p>\n                    <span class=\"text-small text-muted\"\n                      >{{ $t(\"dashboard.created\") }}\n                      {{ formatDate(xtream.user_info.created_at * 1000) }} -\n                      {{ $t(\"dashboard.expiry\") }}\n                      {{\n                        xtream.user_info.exp_date\n                          ? formatDate(xtream.user_info.exp_date * 1000)\n                          : $t(\"dashboard.no-expiry\")\n                      }}</span\n                    >\n                  </div>\n                </a>\n              </perfect-scrollbar>\n            </erd-card-body>\n          </erd-card>\n        </erd-col>\n        <!--  \n\n          XTREAM-ACCOUNT\n\n        -->\n        <erd-modal\n          v-show=\"xtreamAccount.modal\"\n          @close=\"xtreamAccountModal(false)\"\n          :title=\"$t('dashboard.xtream')\"\n          class=\"select-none\"\n          small\n        >\n          <div class=\"px-2\" v-if=\"xtreamAccount.account\">\n            <label class=\"w-100\">{{ $t(\"xtream.url\") }}</label>\n            <erd-input\n              class=\"mt-1 mb-2 w-100\"\n              :value=\"`${xtreamAccount.account.server_info.server_protocol}://${xtreamAccount.account.server_info.url}:${xtreamAccount.account.server_info.port}/get.php?username=${xtreamAccount.account.user_info.username}&password=${xtreamAccount.account.user_info.password}&type=m3u_plus`\"\n              readonly\n            ></erd-input>\n            <erd-row class=\"p-0 m-0\">\n              <erd-col sm=\"8\" class=\"m-0 pl-0 pr-1\">\n                <label class=\"w-100\">{{ $t(\"xtream.host\") }}</label>\n                <erd-input\n                  class=\"mt-1 mb-2 w-100\"\n                  :value=\"xtreamAccount.account.server_info.url\"\n                  readonly\n                ></erd-input>\n              </erd-col>\n              <erd-col sm=\"4\" class=\"m-0 px-0\">\n                <label class=\"w-100\">{{ $t(\"xtream.port\") }}</label>\n                <erd-input\n                  class=\"mt-1 mb-2 w-100\"\n                  :value=\"xtreamAccount.account.server_info.port\"\n                  readonly\n                ></erd-input>\n              </erd-col>\n            </erd-row>\n            <erd-row class=\"p-0 m-0\">\n              <erd-col sm=\"6\" class=\"m-0 pl-0 pr-1\">\n                <label class=\"w-100\">{{ $t(\"xtream.username\") }}</label>\n                <erd-input\n                  class=\"mt-1 mb-2 w-100\"\n                  :value=\"xtreamAccount.account.user_info.username\"\n                  readonly\n                ></erd-input>\n              </erd-col>\n              <erd-col sm=\"6\" class=\"m-0 px-0\">\n                <label class=\"w-100\">{{ $t(\"xtream.password\") }}</label>\n                <erd-input\n                  class=\"mt-1 mb-2 w-100\"\n                  :value=\"xtreamAccount.account.user_info.password\"\n                  readonly\n                ></erd-input>\n              </erd-col>\n            </erd-row>\n            <erd-row class=\"p-0 m-0\">\n              <erd-col sm=\"6\" class=\"m-0 pl-0 pr-1\">\n                <label class=\"w-100\">{{ $t(\"xtream.max-connections\") }}</label>\n                <erd-input\n                  class=\"mt-1 mb-2 w-100\"\n                  :value=\"xtreamAccount.account.user_info.max_connections\"\n                  readonly\n                ></erd-input>\n              </erd-col>\n              <erd-col sm=\"6\" class=\"m-0 px-0\">\n                <label class=\"w-100\">{{\n                  $t(\"xtream.active-connections\")\n                }}</label>\n                <erd-input\n                  class=\"mt-1 mb-2 w-100\"\n                  :value=\"xtreamAccount.account.user_info.active_cons\"\n                  readonly\n                ></erd-input>\n              </erd-col>\n            </erd-row>\n            <erd-row class=\"p-0 m-0\">\n              <erd-col sm=\"6\" class=\"m-0 pl-0 pr-1\">\n                <label class=\"w-100\">{{ $t(\"xtream.created\") }}</label>\n                <erd-input\n                  class=\"mt-1 mb-2 w-100\"\n                  :value=\"\n                    formatDate(\n                      xtreamAccount.account.user_info.created_at * 1000\n                    )\n                  \"\n                  readonly\n                ></erd-input>\n              </erd-col>\n              <erd-col sm=\"6\" class=\"m-0 px-0\">\n                <label class=\"w-100\">{{ $t(\"xtream.expire\") }}</label>\n                <erd-input\n                  class=\"mt-1 mb-2 w-100\"\n                  :value=\"\n                    xtreamAccount.account.user_info.exp_date\n                      ? formatDate(xtreamAccount.account.user_info.exp_date * 1000)\n                      : $t('dashboard.no-expiry')\n                  \"\n                  readonly\n                ></erd-input>\n              </erd-col>\n            </erd-row>\n            <label class=\"w-100\">{{ $t(\"users.status\") }}</label>\n            <erd-input\n              class=\"mt-1 mb-2 w-100\"\n              :value=\"xtreamAccount.account.user_info.status\"\n              readonly\n            ></erd-input>\n            <label class=\"w-100\">{{ $t(\"xtream.message\") }}</label>\n            <erd-input\n              class=\"mt-1 mb-2 w-100\"\n              :value=\"xtreamAccount.account.user_info.message\"\n              readonly\n            ></erd-input>\n          </div>\n          <template v-slot:footer>\n            <erd-button\n              @click=\"xtreamAccountModal(false)\"\n              icon=\"la-times\"\n              class=\"mr-1\"\n              variant=\"danger\"\n              >{{ $t(\"general.close\") }}</erd-button\n            >\n          </template>\n        </erd-modal>\n        <erd-col xl=\"6\" lg=\"12\" xs=\"12\">\n          <erd-card class=\"list-card\">\n            <erd-button\n              class=\"refresh-button\"\n              outline\n              @click=\"loadDashboard\"\n            >\n              <erd-tooltip :tooltip=\"$t('dashboard.tooltip-refresh')\" :enabled=\"showTooltips\">\n                <i class=\"las la-sync\"></i>\n              </erd-tooltip>\n            </erd-button>\n            <erd-card-body>\n              <div class=\"float-left float-none-xs\">\n                <div class=\"d-inline-block\">\n                  <h5 class=\"d-inline\">\n                    {{ $t(\"dashboard.playlist\") }}\n                    <span class=\"text-muted text-small text-capitalize\"></span>\n                  </h5>\n                  <span class=\"text-muted text-small d-block\">{{\n                    $t(\"dashboard.playlist-info\")\n                  }}</span>\n                </div>\n              </div>\n            </erd-card-body>\n            <erd-card-body class=\"list mt-1\">\n              <p\n                class=\"text-muted text-center mt-3\"\n                v-if=\"playlists.length === 0\"\n              >\n                <i class=\"las la-file-video d-block la-3x\"></i>\n                {{ $t(\"dashboard.no-playlists\") }}\n              </p>\n              <perfect-scrollbar>\n                <a\n                  href=\"#\"\n                  class=\"dashboard-list-item\"\n                  v-for=\"(playlist, index) in playlists\"\n                  :key=\"`playlist-${index}`\"\n                  @click.prevent=\"playlistModal(true, playlist)\"\n                >\n                  <i class=\"las la-file-video la-3x\"></i>\n                  <div class=\"details\">\n                    <p\n                      class=\"mt-1 float-right text-small\"\n                      v-if=\"userHasMoviesSeries\"\n                    >\n                      {{\n                        formatNumber(\n                          playlist.live + playlist.movies + playlist.series\n                        )\n                      }}\n                      <i class=\"las la-stream\"></i>\n                    </p>\n                    <p class=\"mt-1 float-right text-small\" v-else>\n                      {{ formatNumber(playlist.live) }}\n                      <i class=\"las la-stream\"></i>\n                    </p>\n                    <p class=\"m-0 p-0\">\n                      <span>{{ playlist.name }}</span>\n                      <span class=\"text-muted text-small ml-2\">\n                        <i class=\"las la-sync\"></i>\n                        {{ formatDate(playlist.synced_at) }}\n                      </span>\n                    </p>\n                    <span\n                      class=\"text-small text-muted\"\n                      v-if=\"userHasMoviesSeries\"\n                    >\n                      {{\n                        $t(\"dashboard.playlist-content\").format(\n                          formatNumber(playlist.live),\n                          formatNumber(playlist.movies),\n                          formatNumber(playlist.series)\n                        )\n                      }}\n                    </span>\n                    <span v-else>\n                      {{\n                        $t(\"dashboard.playlist-content-live\").format(\n                          formatNumber(playlist.live)\n                        )\n                      }}\n                    </span>\n                  </div>\n                </a>\n              </perfect-scrollbar>\n            </erd-card-body>\n          </erd-card>\n        </erd-col>\n        <!--  \n\n          PLAYLIST\n\n        -->\n        <erd-modal\n          v-show=\"playlist.modal\"\n          @close=\"playlistModal(false)\"\n          :title=\"$t('dashboard.playlist')\"\n          class=\"select-none\"\n          small\n        >\n          <div class=\"px-2\" v-if=\"playlist.playlist\">\n            <label class=\"w-100\">{{ $t(\"xtream.name\") }}</label>\n            <erd-input\n              class=\"mt-1 mb-2 w-100\"\n              :value=\"playlist.playlist.name\"\n              readonly\n            ></erd-input>\n            <erd-row class=\"p-0 m-0\">\n              <erd-col sm=\"6\" class=\"m-0 pl-0 pr-1\">\n                <label class=\"w-100\">{{ $t(\"xtream.username\") }}</label>\n                <erd-input\n                  class=\"mt-1 mb-2 w-100\"\n                  :value=\"playlist.playlist.api_username\"\n                  readonly\n                ></erd-input>\n              </erd-col>\n              <erd-col sm=\"6\" class=\"m-0 px-0\">\n                <label class=\"w-100\">{{ $t(\"xtream.password\") }}</label>\n                <erd-input\n                  class=\"mt-1 mb-2 w-100\"\n                  :value=\"playlist.playlist.api_password\"\n                  readonly\n                ></erd-input>\n              </erd-col>\n            </erd-row>\n            <label class=\"w-100\">{{ $t(\"xtream.synced-at\") }}</label>\n            <erd-input\n              class=\"mt-1 mb-2 w-100\"\n              :value=\"formatDate(playlist.playlist.synced_at)\"\n              readonly\n            ></erd-input>\n            <erd-row class=\"p-0 m-0\">\n              <erd-col sm=\"4\" class=\"m-0 pl-0 pr-1\">\n                <label class=\"w-100\">{{ $t(\"xtream.total-live\") }}</label>\n                <erd-input\n                  class=\"mt-1 mb-2 w-100\"\n                  :value=\"formatNumber(playlist.playlist['live'])\"\n                  readonly\n                ></erd-input>\n              </erd-col>\n              <erd-col sm=\"4\" class=\"m-0 pl-0 pr-1\">\n                <label class=\"w-100\">{{ $t(\"xtream.live-new\") }}</label>\n                <erd-input\n                  class=\"mt-1 mb-2 w-100\"\n                  :value=\"formatNumber(playlist.playlist['live-new'])\"\n                  readonly\n                ></erd-input>\n              </erd-col>\n              <erd-col sm=\"4\" class=\"m-0 px-0\">\n                <label class=\"w-100\">{{ $t(\"xtream.live-removed\") }}</label>\n                <erd-input\n                  class=\"mt-1 mb-2 w-100\"\n                  :value=\"formatNumber(playlist.playlist['live-removed'])\"\n                  readonly\n                ></erd-input>\n              </erd-col>\n            </erd-row>\n            <erd-row class=\"p-0 m-0\" v-if=\"userHasMoviesSeries\">\n              <erd-col sm=\"4\" class=\"m-0 pl-0 pr-1\">\n                <label class=\"w-100\">{{ $t(\"xtream.total-movies\") }}</label>\n                <erd-input\n                  class=\"mt-1 mb-2 w-100\"\n                  :value=\"formatNumber(playlist.playlist['movies'])\"\n                  readonly\n                ></erd-input>\n              </erd-col>\n              <erd-col sm=\"4\" class=\"m-0 pl-0 pr-1\">\n                <label class=\"w-100\">{{ $t(\"xtream.movies-new\") }}</label>\n                <erd-input\n                  class=\"mt-1 mb-2 w-100\"\n                  :value=\"formatNumber(playlist.playlist['movie-new'])\"\n                  readonly\n                ></erd-input>\n              </erd-col>\n              <erd-col sm=\"4\" class=\"m-0 px-0\">\n                <label class=\"w-100\">{{ $t(\"xtream.movies-removed\") }}</label>\n                <erd-input\n                  class=\"mt-1 mb-2 w-100\"\n                  :value=\"formatNumber(playlist.playlist['movie-removed'])\"\n                  readonly\n                ></erd-input>\n              </erd-col>\n            </erd-row>\n            <erd-row class=\"p-0 m-0\" v-if=\"userHasMoviesSeries\">\n              <erd-col sm=\"4\" class=\"m-0 pl-0 pr-1\">\n                <label class=\"w-100\">{{ $t(\"xtream.total-series\") }}</label>\n                <erd-input\n                  class=\"mt-1 mb-2 w-100\"\n                  :value=\"formatNumber(playlist.playlist['series'])\"\n                  readonly\n                ></erd-input>\n              </erd-col>\n              <erd-col sm=\"4\" class=\"m-0 pl-0 pr-1\">\n                <label class=\"w-100\">{{ $t(\"xtream.series-new\") }}</label>\n                <erd-input\n                  class=\"mt-1 mb-2 w-100\"\n                  :value=\"formatNumber(playlist.playlist['series-new'])\"\n                  readonly\n                ></erd-input>\n              </erd-col>\n              <erd-col sm=\"4\" class=\"m-0 px-0\">\n                <label class=\"w-100\">{{ $t(\"xtream.series-removed\") }}</label>\n                <erd-input\n                  class=\"mt-1 mb-2 w-100\"\n                  :value=\"formatNumber(playlist.playlist['series-removed'])\"\n                  readonly\n                ></erd-input>\n              </erd-col>\n            </erd-row>\n          </div>\n          <template v-slot:footer>\n            <erd-button\n              @click=\"playlistModal(false)\"\n              icon=\"la-times\"\n              class=\"mr-1\"\n              variant=\"danger\"\n              >{{ $t(\"general.close\") }}</erd-button\n            >\n          </template>\n        </erd-modal>\n      </erd-row>\n    </erd-col>\n    <erd-col xl=\"12\" xs=\"12\" class=\"m-0 p-0\" v-if=\"userHasMoviesSeries\">\n      <erd-row class=\"m-0 p-0\">\n        <erd-col xl=\"3\" md=\"6\">\n          <erd-card>\n            <erd-card-body class=\"dashboard-card\">\n              <i class=\"las la-list la-5x color-theme-1\"></i>\n              <div class=\"dashboard-card-title\" style=\"\">\n                <h5 class=\"m-0\">{{ $t(\"dashboard.playlist\") }}</h5>\n                <p class=\"m-0 color-theme-1\">\n                  {{ formatNumber(statistics.playlists) }}\n                </p>\n              </div>\n            </erd-card-body>\n          </erd-card>\n        </erd-col>\n        <erd-col xl=\"3\" md=\"6\">\n          <erd-card>\n            <erd-card-body class=\"dashboard-card\">\n              <i class=\"las la-tv la-5x color-theme-1\"></i>\n              <div class=\"dashboard-card-title\" style=\"\">\n                <h5 class=\"m-0\">{{ $t(\"dashboard.live-streams\") }}</h5>\n                <p class=\"m-0 color-theme-1\">\n                  {{ formatNumber(statistics.live) }}\n                </p>\n              </div>\n            </erd-card-body>\n          </erd-card>\n        </erd-col>\n        <erd-col xl=\"3\" md=\"6\">\n          <erd-card>\n            <erd-card-body class=\"dashboard-card\">\n              <i class=\"las la-film la-5x color-theme-1\"></i>\n              <div class=\"dashboard-card-title\" style=\"\">\n                <h5 class=\"m-0\">{{ $t(\"dashboard.movies\") }}</h5>\n                <p class=\"m-0 color-theme-1\">\n                  {{ formatNumber(statistics.movies) }}\n                </p>\n              </div>\n            </erd-card-body>\n          </erd-card>\n        </erd-col>\n        <erd-col xl=\"3\" md=\"6\">\n          <erd-card>\n            <erd-card-body class=\"dashboard-card\">\n              <i class=\"las la-video la-5x color-theme-1\"></i>\n              <div class=\"dashboard-card-title\" style=\"\">\n                <h5 class=\"m-0\">{{ $t(\"dashboard.series\") }}</h5>\n                <p class=\"m-0 color-theme-1\">\n                  {{ formatNumber(statistics.series) }}\n                </p>\n              </div>\n            </erd-card-body>\n          </erd-card>\n        </erd-col>\n      </erd-row>\n    </erd-col>\n    <erd-col xl=\"12\" xs=\"12\" class=\"m-0 p-0\" v-if=\"!userHasMoviesSeries\">\n      <erd-row class=\"m-0 p-0\">\n        <erd-col xl=\"6\" md=\"6\">\n          <erd-card>\n            <erd-card-body class=\"dashboard-card\">\n              <i class=\"las la-list la-5x color-theme-1\"></i>\n              <div class=\"dashboard-card-title\" style=\"\">\n                <h5 class=\"m-0\">{{ $t(\"dashboard.playlist\") }}</h5>\n                <p class=\"m-0 color-theme-1\">\n                  {{ formatNumber(statistics.playlists) }}\n                </p>\n              </div>\n            </erd-card-body>\n          </erd-card>\n        </erd-col>\n        <erd-col xl=\"6\" md=\"6\">\n          <erd-card>\n            <erd-card-body class=\"dashboard-card\">\n              <i class=\"las la-tv la-5x color-theme-1\"></i>\n              <div class=\"dashboard-card-title\" style=\"\">\n                <h5 class=\"m-0\">{{ $t(\"dashboard.live-streams\") }}</h5>\n                <p class=\"m-0 color-theme-1\">\n                  {{ formatNumber(statistics.live) }}\n                </p>\n              </div>\n            </erd-card-body>\n          </erd-card>\n        </erd-col>\n      </erd-row>\n    </erd-col>\n    <erd-col xl=\"12\" xs=\"12\" class=\"m-0 p-0\">\n      <erd-row class=\"m-0 p-0\">\n        <erd-col xl=\"6\" lg=\"12\" xs=\"12\">\n          <erd-card class=\"list-card\">\n            <erd-button\n              class=\"refresh-button\"\n              outline\n              @click=\"loadTickets\"\n            >\n              <erd-tooltip :tooltip=\"$t('dashboard.tooltip-refresh')\" :enabled=\"showTooltips\">\n                <i class=\"las la-sync\"></i>\n              </erd-tooltip>\n            </erd-button>\n            <erd-card-body>\n              <div class=\"float-left float-none-xs\">\n                <div class=\"d-inline-block\">\n                  <h5 class=\"d-inline\">\n                    {{ $t(\"dashboard.tickets\") }}\n                    <span class=\"text-muted text-small text-capitalize\"></span>\n                  </h5>\n                  <span class=\"text-muted text-small d-block\">{{\n                    $t(\"dashboard.tickets-info\")\n                  }}</span>\n                </div>\n              </div>\n            </erd-card-body>\n            <erd-card-body class=\"list mt-1\">\n              <p\n                class=\"text-muted text-center mt-3\"\n                v-if=\"tickets.length === 0\"\n              >\n                <i class=\"las la-headset d-block la-3x\"></i>\n                {{ $t(\"dashboard.no-tickets\") }}\n              </p>\n              <perfect-scrollbar>\n                <router-link\n                  class=\"dashboard-list-item\"\n                  v-for=\"(ticket, index) in tickets\"\n                  :key=\"`ticket-${index}`\"\n                  to=\"/app/user/tickets\"\n                >\n                  <i\n                    class=\"las la-3x\"\n                    :class=\"ticketIcon(ticket.ticket_status)\"\n                  ></i>\n                  <div class=\"details\">\n                    <p class=\"mt-1 float-right text-small\">\n                      {{ $t(`departement.${ticket.departement}`) }}\n                      <i class=\"las la-headset\"></i>\n                    </p>\n                    <p class=\"m-0 p-0\">\n                      <span>{{ ticket.title }}</span>\n                      <span class=\"text-muted text-small ml-2\">{{\n                        $t(`priority.${ticket.priority}`)\n                      }}</span>\n                    </p>\n                    <span class=\"text-small text-muted\"\n                      >{{ formatDate(ticket.created) }} -\n                      {{ formatDate(ticket.last_updated) }}</span\n                    >\n                  </div>\n                </router-link>\n              </perfect-scrollbar>\n            </erd-card-body>\n          </erd-card>\n        </erd-col>\n        <erd-col xl=\"6\" lg=\"12\" xs=\"12\">\n          <erd-card class=\"list-card\">\n            <erd-button\n              class=\"refresh-button\"\n              outline\n              @click=\"loadInvoices\"\n            >\n              <erd-tooltip :tooltip=\"$t('dashboard.tooltip-refresh')\" :enabled=\"showTooltips\">\n                <i class=\"las la-sync\"></i>\n              </erd-tooltip>\n            </erd-button>\n            <erd-card-body>\n              <div class=\"float-left float-none-xs\">\n                <div class=\"d-inline-block\">\n                  <h5 class=\"d-inline\">\n                    {{ $t(\"dashboard.invoices\") }}\n                    <span class=\"text-muted text-small text-capitalize\"></span>\n                  </h5>\n                  <span class=\"text-muted text-small d-block\">{{\n                    $t(\"dashboard.invoices-info\")\n                  }}</span>\n                </div>\n              </div>\n            </erd-card-body>\n            <erd-card-body class=\"list mt-1\">\n              <p\n                class=\"text-muted text-center mt-3\"\n                v-if=\"invoices.length === 0\"\n              >\n                <i class=\"las la-receipt d-block la-3x\"></i>\n                {{ $t(\"dashboard.no-invoices\") }}\n              </p>\n              <perfect-scrollbar>\n                <router-link\n                  class=\"dashboard-list-item\"\n                  v-for=\"(invoice, index) in invoices\"\n                  :key=\"`invoice-${index}`\"\n                  to=\"/app/user/invoices\"\n                >\n                  <i class=\"las la-receipt la-3x\"></i>\n                  <div class=\"details\">\n                    <p class=\"mt-1 float-right text-small\">\n                      {{ invoicePrice(invoice) }}\n                      <i class=\"las la-euro-sign\"></i>\n                    </p>\n                    <p class=\"m-0 p-0\">{{ invoiceDescription(invoice) }}</p>\n                    <span class=\"text-small text-muted\">\n                      {{ formatDate(invoice.invoice_date) }}\n                    </span>\n                  </div>\n                </router-link>\n              </perfect-scrollbar>\n            </erd-card-body>\n          </erd-card>\n        </erd-col>\n      </erd-row>\n    </erd-col>\n  </erd-row>\n</template>\n\n<script>\nimport { EventBus } from \"../../../services/ebus\";\nimport { mapActions, mapGetters } from \"vuex\";\nimport { httpService } from \"../../../services/http\";\n\nexport default {\n  data() {\n    return {\n      isLoading: false,\n      xtreamAccount: {\n        modal: false,\n        account: null,\n      },\n      playlist: {\n        modal: false,\n        playlist: null,\n      },\n      xtreamAccounts: [],\n      playlists: [],\n      tickets: [],\n      invoices: [],\n      statistics: {\n        playlists: 0,\n        live: 0,\n        movies: 0,\n        series: 0,\n      },\n    };\n  },\n  computed: {\n    ...mapGetters([\"currentUser\", \"settings\"]),\n    showTooltips () {\n      return this.settings.showTooltips;\n    },\n    userHasMoviesSeries() {\n      return (\n        this.currentUser.subscription &&\n        this.currentUser.subscription.subscription_type > 0\n      );\n    },\n    memberSince() {\n      return new Date(this.currentUser.user.created).toLocaleDateString(\n        this.$t(\"date.locale\")\n      );\n    },\n    memberStatus() {\n      return this.currentUser.subscription\n        ? this.$t(`status.${this.currentUser.subscription.subscription_type}`)\n        : this.$t(\"status.none\");\n    },\n    userRole() {\n      return this.currentUser.user\n        ? this.$t(`roles.${this.currentUser.user.role}`)\n        : this.$t(\"roles.none\");\n    },\n    nextInvoiceExpired() {\n      return this.currentUser.subscription &&\n        this.currentUser.subscription.end_date\n        ? new Date(this.currentUser.subscription.end_date).getTime() <\n            new Date().getTime()\n        : false;\n    },\n    nextInvoiceDate() {\n      return this.currentUser.subscription &&\n        this.currentUser.subscription.end_date\n        ? new Date(this.currentUser.subscription.end_date).toLocaleDateString(\n            this.$t(\"date.locale\")\n          )\n        : this.$t(\"general.not-available\");\n    },\n  },\n  methods: {\n    ...mapActions([]),\n    pagePrint() {\n      document.getElementById(\"dashboard\").print();\n    },\n    formatDate(date) {\n      return new Date(date).toLocaleDateString(\n        this.$t(\"date.locale\"),\n        this.dateOptions\n      );\n    },\n    loadDashboard() {\n      this.isLoading = true;\n      this.playlists = [];\n      this.xtreamAccounts = [];\n      Promise.all([\n        httpService.get(\"dashboard/statistics\"),\n        httpService.get(\"dashboard/xtream-accounts\"),\n        httpService.get(\"dashboard/playlists\"),\n      ])\n        .then((res) => {\n          if (res[0].status === true) {\n            this.statistics.playlists = res[0].data.playlist;\n            this.statistics.live = res[0].data.live;\n            this.statistics.movies = res[0].data.movies;\n            this.statistics.series = res[0].data.series;\n          }\n          if (res[1].status === true) {\n            this.xtreamAccounts = res[1].data;\n          }\n          if (res[2].status === true) {\n            this.playlists = res[2].data;\n          }\n          this.isLoading = false;\n        })\n        .catch(() => {\n          this.isLoading = false;\n          this.$notify(\n            \"error\",\n            this.$t(\"profile.failed\"),\n            this.$t(\"dashboard.load-error\"),\n            \"la-user-shield\",\n            { duration: 5000, permanent: false }\n          );\n        });\n    },\n    loadTickets() {\n      httpService\n        .get(\"users/tickets\")\n        .then((res) => {\n          if (res.status === true) {\n            this.tickets = res.data;\n          }\n        })\n        .catch(() => {\n          this.$notify(\n            \"error\",\n            this.$t(\"profile.failed\"),\n            this.$t(\"tickets.tickets-loading-failed\"),\n            \"la-user-shield\",\n            { duration: 5000, permanent: false }\n          );\n        });\n    },\n    loadInvoices() {\n      httpService\n        .get(\"users/invoices\")\n        .then((res) => {\n          if (res.status === true) {\n            this.invoices = res.data;\n          }\n        })\n        .catch(() => {\n          this.$notify(\n            \"error\",\n            this.$t(\"profile.failed\"),\n            this.$t(\"subscriptions.invoice-error\"),\n            \"la-user-shield\",\n            { duration: 5000, permanent: false }\n          );\n        });\n    },\n    ticketIcon(status) {\n      switch (status) {\n        case 0:\n          return \"la-user\";\n        case 1:\n          return \"la-user-check\";\n        case 2:\n          return \"la-user-check\";\n        case 3:\n          return \"la-user-lock\";\n      }\n    },\n    formatNumber(num) {\n      return new Intl.NumberFormat(this.$t(\"date.locale\")).format(num);\n    },\n    formatCurrency(num) {\n      return new Intl.NumberFormat(this.$t(\"date.locale\"), {\n        style: \"currency\",\n        currency: \"EUR\",\n      }).format(num);\n    },\n    parsePrice(price) {\n      return parseFloat(price.replace(\",\", \".\"));\n    },\n    invoicePrice(invoice) {\n      let total = 0;\n      invoice.items.map((item) => {\n        total += item.amount * this.parsePrice(item.price);\n      });\n      return this.formatCurrency(total);\n    },\n    invoiceDescription(invoice) {\n      return invoice.items.length\n        ? invoice.items[0].description\n        : this.$t(\"general.not-available\");\n    },\n    xtreamAccountModal(show, account) {\n      if (account) {\n        this.xtreamAccount.account = account;\n      }\n      this.xtreamAccount.modal = show;\n    },\n    playlistModal(show, playlist) {\n      if (playlist) {\n        this.playlist.playlist = playlist;\n      }\n      this.playlist.modal = show;\n    },\n  },\n  beforeMount() {\n    this.loadDashboard();\n    this.loadTickets();\n    this.loadInvoices();\n    EventBus.$on(\"print\", this.pagePrint);\n  },\n  beforeDestroy() {\n    EventBus.$off(\"print\", this.pagePrint);\n  },\n};\n</script>\n\n<style scoped lang=\"scss\">\n.dashboard-card {\n  display: table;\n}\n.dashboard-card-title {\n  display: table-cell;\n  vertical-align: middle;\n  width: 100%;\n  padding-left: 0.5rem;\n}\n.list-card {\n  user-select: none;\n  position: relative;\n  height: 340px;\n\n  .refresh-button {\n    position: absolute;\n    right: 20px;\n    top: 20px;\n    border-radius: 50%;\n    height: 32px;\n    width: 32px;\n    line-height: 32px;\n    padding: 0;\n  }\n\n  .ps {\n    height: 230px;\n    padding-right: 1rem;\n  }\n\n  .list {\n    height: 200px;\n  }\n\n  .dashboard-list-item {\n    display: table;\n    padding-bottom: 8px;\n\n    &:not(:first-child) {\n      padding-top: 8px;\n    }\n\n    &:not(:last-child) {\n      border-bottom: 1px solid $separator-color-light;\n    }\n\n    .details {\n      display: table-cell;\n      vertical-align: middle;\n      width: 100%;\n      padding-left: 0.25rem;\n    }\n  }\n}\n</style>\n"]}]}