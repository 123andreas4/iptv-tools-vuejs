<home-layout>\n    <nav class=\"navbar d-print-none\">\n      <div class=\"navbar-side navbar-nav-homepage\">\n        <a href=\"#\">Link 1</a>\n        <a href=\"#\">Link 2</a>\n        <a href=\"#\">Link 3</a>\n      </div>\n      <a href=\"/\" class=\"navbar-logo active\">\n        <span class=\"logo d-none d-xs-block\"></span>\n        <span class=\"logo-mobile d-block d-xs-none\"></span>\n      </a>\n      <div class=\"navbar-side right navbar-side-homepage\">\n        <erd-button>{{ $t(\"landing.login\") }}</erd-button>\n      </div>\n    </nav>\n    <erd-row>\n      <erd-col xxl=\"12\" sm=\"12\" class=\"p-0 mx-0 mt-4\">\n        \n          <div class=\"hero-sass hero-swiper-btn\" id=\"home\">\n              <div class=\"container_f1\">\n                  <div class=\"swiper-wrapper\">\n                      <swiper :options=\"swiperOption\">\n                          <div class=\"swiper-slide\" :style=\"bgImg\">\n                              <div class=\"hero-item text-center\">\n                                  <div class=\"hero-content mt-2\">\n                                      <h2 class=\"h1 hero-content-title font-weight-light\">{{ $t(\"hero.title-1\") }}</h2>\n                                  </div>\n                                  <div class=\"hero-thumb\">\n                                      <img src=\"@/assets/hero/hero-sass/sass-thumb-1.png\" class=\"img-fluid\" alt=\"hero thumb\">\n                                  </div>\n                              </div>\n                          </div>\n                          <div class=\"swiper-slide\" :style=\"bgImg\">\n                              <div class=\"hero-item text-center\">\n                                  <div class=\"hero-content mt-2\">\n                                      <h2 class=\"h1 hero-content-title font-weight-light\">{{ $t(\"hero.title-2\") }}</h2>\n                                  </div>\n                                  <div class=\"hero-thumb\">\n          